import java.util.*;

public class ArrayFrontBackCappedList<T> implements FrontBackCappedListInterface<T> {

    private T [] list;
    private int numberOfElements;

    public ArrayFrontBackCappedList(int capacity) {
        this.list = (T[]) new Object[capacity];
        this.numberOfElements = 0;
    }

    @Override
    public boolean addFront(T newEntry) {
        if (isFull()) {
            return false;
        }
        else {
            for (int i = numberOfElements-1; i >= 0; i--) {
                list[i+1] = list[i];
            }
            list[0] = newEntry;
            numberOfElements++;
            return true;
        }
    }

    @Override
    public boolean addBack(T newEntry) {
        if (isFull()) {
            return false;
        }
        else {
            list[numberOfElements] = newEntry;
            numberOfElements++;
            return true;
        }
    }

    public T removeFront() {
        // Code here
        return null;
    }

    public T removeBack() {
        // Code here
        return null;
    }


    @Override
    public String toString() {
        T[] tempList = (T[]) new Object[numberOfElements];
        T temp;
        if(!isEmpty()){
            for (int i = 0; i < numberOfElements; i++) {
                tempList[i] = list[i];
            }
        }
        return "size=" + this.size() + ";" +
                " capacity=" + this.list.length + "; " +
                Arrays.toString(tempList);
    }

    @Override
    public void clear() {
        for(int i =0; i<=list.length-1 ; i++){
            list[i] = null;
        }
        numberOfElements = 0;
    }

    @Override
    public T getEntry(int givenPosition) {
        return null;
    }

    @Override
    public int indexOf(T anEntry) {
        return 0;
    }

    @Override
    public int lastIndexOf(T anEntry) {
        return 0;
    }

    @Override
    public boolean contains(T anEntry) {
        boolean found = false;
        int index = 0;
        while (!found && (index < numberOfElements)) {
            if (anEntry.equals(list[index]))
                found = true;
            index++;
        }
        return found;
    }

    @Override
    public boolean isEmpty() {
        boolean result;
        if(size() ==0){
            result = true;
        }
        else {
            result = false;
        }
        return result;
    }

    @Override
    public boolean isFull() {
        boolean result = true;
        if(isEmpty()){
            result = false;
        }
        else{
            for(int i = 0; i<= list.length-1; i++){
                if(list[i] == null){
                    result = false;
                }
            }
        }
        return result;
    }

    @Override
    public int size() {
        return this.numberOfElements;
    }

    private boolean checkCapacity(int capacity) {
        if (capacity >= this.list.length) {
            return true;
        }
        else {
            return false;
        }
    }

}
